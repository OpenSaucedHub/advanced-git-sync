{
  "name": "@opensaucedhub/git-sync",
  "description": "Synchronize GitHub repository with GitLab including branches, PRs, and issues",
  "version": "1.5.5",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "provenance": true
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "f": "prettier --write \"**/*.*\"",
    "start": "bun src/index.ts",
    "check": "tsc --noEmit",
    "build": "ncc build src/index.ts -o dist"
  },
  "keywords": [
    "github",
    "actions",
    "gitlab",
    "automation",
    "git"
  ],
  "author": "vikshan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenSaucedHub/advanced-git-sync.git"
  },
  "bugs": {
    "url": "https://github.com/OpenSaucedHub/advanced-git-sync/issues"
  },
  "homepage": "https://github.com/OpenSaucedHub/advanced-git-sync#readme",
  "dependencies": {
    "@actions/core": "^3.0.0",
    "@actions/github": "^9.0.0",
    "@gitbeaker/rest": "^43.8.0",
    "@octokit/rest": "^22.0.1",
    "deepmerge": "^4.3.1",
    "js-yaml": "^4.1.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.6",
    "@semantic-release/npm": "^13.1.4",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vercel/ncc": "^0.38.4",
    "axios": "^1.13.5",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "conventional-changelog-gitmoji": "^0.2.5",
    "eslint": "^10.0.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "prettier": "^3.8.1",
    "semantic-release": "^25.0.3",
    "semantic-release-gitmoji": "^1.6.9",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  }
}
